{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/orm": "~2.6",
        "openapi/server-bundle": "dev-master",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.2",
        "symfony/templating": "^4.2",
        "symfony/twig-pack": "^1.0",
        "symfony/yaml": "^4.2",
        "twig/twig": "^2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.2"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.11",
        "symfony/web-server-bundle": "^4.2"
    },
    "repositories": [{
       "type": "path",
       "url": "../OpenAPIServerBundle/",
       "options": {
               "symlink": false
       }
    }]
}
